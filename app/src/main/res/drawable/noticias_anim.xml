<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:name="vector"
            android:width="24dp"
            android:height="24dp"
            android:viewportWidth="24"
            android:viewportHeight="24">
            <path
                android:name="libro_outline"
                android:pathData="M 3 8 L 3 19 C 6.48 19 9.64 20.351 12 22.551 C 14.36 20.361 17.52 19 21 19 L 21 8 C 17.52 8 14.36 9.351 12 11.551 C 9.64 9.351 6.48 8 3 8 Z M 5 10.17 C 7.1 10.55 9.051 11.52 10.641 13 L 12 14.279 L 13.359 13.01 C 14.949 11.53 16.9 10.56 19 10.18 L 19 17.131 C 16.47 17.471 14.07 18.429 12 19.949 C 9.94 18.429 7.53 17.459 5 17.119 Z"
                android:fillColor="#000"
                android:strokeWidth="1"/>
            <group android:name="group">
                <path
                    android:name="circulo_outline"
                    android:pathData="M 12 9 C 14.21 9 16 7.21 16 5 C 16 2.79 14.21 1 12 1 C 9.79 1 8 2.79 8 5 C 8 7.21 9.79 9 12 9 Z M 12 3 C 13.1 3 14 3.9 14 5 C 14 6.1 13.1 7 12 7 C 10.9 7 10 6.1 10 5 C 10 3.9 10.9 3 12 3 Z"
                    android:fillColor="#000"
                    android:strokeWidth="1"/>
            </group>
            <group android:name="group_1">
                <path
                    android:name="circulo_baseline"
                    android:pathData="M 12 8 C 13.66 8 15 6.66 15 5 C 15 3.34 13.66 2 12 2 C 10.34 2 9 3.34 9 5 C 9 6.66 10.34 8 12 8 Z"
                    android:fillColor="#000"
                    android:fillAlpha="0"
                    android:strokeWidth="1"/>
            </group>
            <path
                android:name="libro_baseline"
                android:pathData="M 12 11.55 C 9.64 9.35 6.48 8 3 8 L 3 19 C 6.48 19 9.64 20.35 12 22.55 C 14.36 20.36 17.52 19 21 19 L 21 8 C 17.52 8 14.36 9.35 12 11.55 Z"
                android:fillColor="#000"
                android:fillAlpha="0"
                android:strokeWidth="1"/>
        </vector>
    </aapt:attr>
    <target android:name="group">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="translateY"
                android:duration="400"
                android:valueFrom="0"
                android:valueTo="10"
                android:valueType="floatType"
                android:interpolator="@android:anim/anticipate_overshoot_interpolator"/>
        </aapt:attr>
    </target>
    <target android:name="group_1">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="translateY"
                    android:duration="300"
                    android:valueFrom="0"
                    android:valueTo="10"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/anticipate_overshoot_interpolator"/>
                <objectAnimator
                    android:propertyName="translateY"
                    android:startOffset="300"
                    android:duration="400"
                    android:valueFrom="10"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/anticipate_overshoot_interpolator"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="circulo_outline">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="fillAlpha"
                android:startOffset="200"
                android:duration="200"
                android:valueFrom="1"
                android:valueTo="0"
                android:valueType="floatType"
                android:interpolator="@android:interpolator/fast_out_slow_in"/>
        </aapt:attr>
    </target>
    <target android:name="circulo_baseline">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="fillAlpha"
                android:startOffset="300"
                android:duration="100"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType"
                android:interpolator="@android:interpolator/fast_out_slow_in"/>
        </aapt:attr>
    </target>
    <target android:name="libro_outline">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="fillAlpha"
                android:duration="400"
                android:valueFrom="1"
                android:valueTo="1"
                android:valueType="floatType"
                android:interpolator="@android:interpolator/fast_out_slow_in"/>
        </aapt:attr>
    </target>
    <target android:name="libro_baseline">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="fillAlpha"
                android:duration="300"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType"
                android:interpolator="@android:interpolator/fast_out_slow_in"/>
        </aapt:attr>
    </target>
</animated-vector>
